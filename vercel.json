{
    "buildCommand": "npm run tina:build",
    "outputDirectory": "dist",
    "framework": "astro",
    "devCommand": "npm run tina:dev",
    "rewrites": [
      {
        "source": "/admin",
        "destination": "/admin/index.html"
      },
      {
        "source": "/admin/:path*",
        "destination": "/admin/:path*"
      }
    ]
  }